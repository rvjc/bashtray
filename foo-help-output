
COMMAND
	foo - set up a new data directory

USAGE
	foo [options] [ <badger> ]

REQUIRED
	badger                      badger number

OPTIONS
	--key                       use key

	-n, --normal <value>        normal number

	-m, --magic <value>         magic number

	--quiet                     suppress all output except errors

	--verbose                   show detailed output

	--version                   show version information (standalone option)

	--help                      show help information (standalone option)

	--exitcodes                 show exit codes (standalone option)

